{
  "preset": "laravel",
  "rules": {
    "ordered_imports": {
      "sort_algorithm": "length"
    },
    "function_declaration": {
      "closure_fn_spacing": "none"
    },
    "concat_space": {
      "spacing": "one"
    },
    "not_operator_with_successor_space": false,
    "no_spaces_after_function_name": true
  }
}
